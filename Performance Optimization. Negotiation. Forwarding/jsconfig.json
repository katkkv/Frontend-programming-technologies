{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2016",
        "jsx": "preserve",
        "checkJs": true,
        "baseUrl": "./",
        "paths": {
            "#assets/*": ["./src/assets/*"],
            "#utils/*": ["./src/utils/*"],
            "#store/*": ["./src/store/*"],
            "#components/*": ["./src/components/*"],
            "#pages/*": ["./src/pages/*"]
        }
    },
    "exclude": ["node_modules", "**/node_modules/*"]
}